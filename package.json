{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "Thids repository is for exploring an MCP (Model Context Protocol) server implementation using Node.js and TypeScript.",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "server:build": "tsc && chmod 755 build/index.js",
    "server:start": "node build/index.js",
    "server:dev": "tsx src/index.ts",
    "server:inspect": "set DANGEROUSLY_OMIT_AUTH=true && npx @modelcontextprotocol/inspector npm run server:dev"
  },
  "bin": {
    "mcp-server": "./build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AneshSM/mcp-server.git"
  },
  "keywords": [],
  "author": "Anesh Somanath Majalikar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AneshSM/mcp-server/issues"
  },
  "homepage": "https://github.com/AneshSM/mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@types/express": "^5.0.3",
    "express": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
